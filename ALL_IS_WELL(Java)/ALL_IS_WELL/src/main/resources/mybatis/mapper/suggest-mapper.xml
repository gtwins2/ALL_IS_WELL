<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="suggest">
	<!-- 공지사항 조회 -->
	<select id="suggestList" resultType="com.kh.app.board.vo.NoticeVo">
		SELECT 
			NO
			,MEMBER_NO
			,TITLE
			,CONTENT
			,ENROLL_DATE
			,MODIFY_DATE
			,SUGGESTION_STATUS
		FROM SUGGESTIONS
		
	</select>
	<select id="getListCnt" resultType="int">
		SELECT COUNT(*)
		FROM SUGGESTIONS
	</select>
	
</mapper>