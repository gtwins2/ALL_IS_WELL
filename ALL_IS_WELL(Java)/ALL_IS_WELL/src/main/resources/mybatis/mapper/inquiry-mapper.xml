<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="inquiry">
	<!-- 공지사항 조회 -->
	<select id="inquiryList" resultType="com.kh.app.board.vo.InquiryVo">
		SELECT 
			NO
			,MEMBER_NO
			,TITLE
			,CONTENT
			,ENROLL_DATE
			,MODIFY_DATE
			,INQUIRY_STATUS
		FROM INQUIRY
		
	</select>
	
	
</mapper>