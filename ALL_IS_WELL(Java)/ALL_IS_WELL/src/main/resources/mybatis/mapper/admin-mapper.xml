<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin">
	<select id="login" resultType="com.kh.app.admin.vo.AdminVo">
		SELECT 
			NO
			,ADMIN_ID
			,ADMIN_PWD
			,ADMIN_NICK
		FROM ADMIN
		WHERE ID = #{id}
		
	</select>
</mapper>