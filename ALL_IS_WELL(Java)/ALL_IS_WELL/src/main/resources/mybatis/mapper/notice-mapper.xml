<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="notice">
	<!-- 공지사항 조회 -->
	<select id="noticeList" resultType="com.kh.app.board.vo.NoticeVo">
		SELECT 
			NO
			,WRITER_NO
			,TITLE
			,CONTENT
			,ENROLL_DATE
			,MODIFY_DATE
			,STATUS
		FROM NOTICE
		
	</select>
	
	
</mapper>